<% @songs.each do |song|%>
  <div id="song_<%= song.id %>_info">
    <h2><%= link_to song.title, "/songs/#{song.id}" %></h2>
    <p>Play Count: <%= song.play_count %></p>
    <p>Length: <%= song.length %></p>
    <% if current_admin? %>
      <%= button_to "delete", admin_song(song), method: :delete %>
    <% end %>
  </div>
<% end %>
